    "skipThirdPartyRequests": true,
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "cacheAjaxRequests": false,
    "source": "../build",
    "include": [
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11",
      "not dead"
    ]
  }
